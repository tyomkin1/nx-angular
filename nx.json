{
  "npmScope": "svvs",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "backend-api": {
      "tags": ["type:app"]
    },
    "frontend-admin": {
      "tags": ["scope:frontend-admin", "type:app"]
    },
    "frontend-client": {
      "tags": ["scope:frontend-client", "type:app" ]
    },
    "frontend-client-e2e": {
      "tags": [],
      "implicitDependencies": ["frontend-client"]
    },
    "frontend/admin-e2e": {
      "tags": [],
      "implicitDependencies": ["frontend-admin"]
    }
  }
}
